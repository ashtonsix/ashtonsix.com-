    "now-build": "echo \"Build before deploying!\"",
    "watch": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run watch",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
